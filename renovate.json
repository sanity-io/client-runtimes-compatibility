{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>sanity-io/renovate-presets//ecosystem/auto"],
  "ignorePaths":[
      "runtimes/bun-script/yarn.lock"
  ],
  "packageRules": [
    {
      "packageNames": ["skunkworks-test-client"],
      "rangeStrategy": "pin",
      "automerge": true,
      "semanticCommitType": "chore",
      "schedule": ["at any time"]
    },
    {
      "packageNames": ["node-fetch"],
      "followTag": "cjs",
      "rangeStrategy": "pin"
    },
    {
      "packageNames": ["@sanity/client"],
      "followTag": "esm",
      "rangeStrategy": "pin"
    }
  ]
}
